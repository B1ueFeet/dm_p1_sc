<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Test" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
         <item name="colorPrimary">@color/my_light_primary</item>
    </style>

    <style name="button_theme" >
        <item name="android:backgroundTint">@color/title</item>
        <item name="android:textSize">25sp</item>
<!--        <item name="android:fontFamily">@font/pacifico</item>-->

    </style>
    <style name="edit_text_theme">
        <item name="android:layout_marginBottom">@dimen/et_normal_size</item>
        <item name="android:layout_marginHorizontal">@dimen/et_normal_margin</item>
        <item name="android:textColorHint">@color/white</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:background">@drawable/et_shape_background</item>
        <item name="android:textSize">@dimen/text_normal_size</item>
        <item name="android:paddingLeft">@dimen/padding_left</item>
